tify-content: center;\r\n  align-items: center;\r\n  margin: 5px 5%;\r\n\r\n  ul {\r\n    list-style: none;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n  }\r\n\r\n  .item {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n    padding-top: 10px;\r\n\r\n    .image {\r\n      height: 200px;\r\n      width: 400px;\r\n      background-color: #fff;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      overflow: hidden;\r\n    }\r\n\r\n    img {\r\n      border-radius: 4px;\r\n      width: 100%;\r\n      object-fit: contain;\r\n    }\r\n\r\n    p.hidden {\r\n      display: none;\r\n    }\r\n  }\r\n\r\n  li {\r\n    flex-basis: 49%;\r\n    padding: 10px;\r\n    background: #fff;\r\n    color: rgba(154, 35, 245, 1);\r\n\r\n    height: auto;\r\n    border: 1px solid rgba(245, 147, 35, 1);\r\n\r\n    .name {\r\n      // display: flex;\r\n      align-items: center;\r\n      font-size: 22px;\r\n      margin-top: 10px;\r\n      text-align: center;\r\n      width: 100%;\r\n\r\n      div {\r\n        margin: 10px 0;\r\n        padding: 0 10px;\r\n      }\r\n\r\n      .arrow {\r\n        transition: 1s;\r\n        cursor: pointer;\r\n      }\r\n    }\r\n\r\n    .description,\r\n    p {\r\n      font-size: 18px;\r\n      width: 100%;\r\n      text-align: center;\r\n      margin: 20px 0;\r\n      width: 90%;\r\n      display: inline;\r\n      height: auto;\r\n    }\r\n\r\n    .sizes {\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      flex-wrap: wrap;\r\n      border-top: 1px solid rgba(154, 35, 245, 1);\r\n      text-transform: capitalize;\r\n\r\n      p {\r\n        font-size: 20px;\r\n        margin: 10px 0;\r\n      }\r\n\r\n      form {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        align-items: center;\r\n        justify-content: center;\r\n        flex-basis: auto;\r\n        list-style: none;\r\n      }\